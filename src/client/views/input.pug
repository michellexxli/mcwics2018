	script.
		$(document).ready(function() {
			$('#calendar').fullCalendar({
			selectable: true,
			unselectAuto: false,
			header: {
				left: 'prev,next today',
				center: 'title',
				right: 'month,agendaWeek,agendaDay'
			},
			dayClick: function(date) {
				document.getElementById("startdate").innerHTML = date.format();
				console.log(document.getElementById("startdate").innerHTML);
				document.getElementById("enddate").innerHTML = date.format();
				console.log(document.getElementById("enddate").innerHTML);
			},
			select: function(startDate, endDate) {
				document.getElementById("startdate").innerHTML = startDate.format();
				document.getElementById("enddate").innerHTML = endDate.format();
			}
			});
		});
